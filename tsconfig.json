{
  "$schema": "https://raw.githubusercontent.com/TypeScriptToLua/TypeScriptToLua/master/tsconfig-schema.json",
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext"],
    "moduleResolution": "Node",
    "types": ["lua-types/5.3"],
    "strict": true,
    "sourceMap": true,
    "outDir": "Lua"
  },
  "tstl": {
    "luaTarget": "5.3",
    "buildMode": "library"
  }
}
